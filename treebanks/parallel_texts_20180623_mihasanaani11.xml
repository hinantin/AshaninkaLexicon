<parallel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="parallel_texts.xsd">
    <text id="1.0">
        <sentence lang="prq">APIINKA</sentence>
        <sentence lang="es">EL SUCESOR DE INCA</sentence>
        <morphology format="ashaninkamorph" analysislevel="not-fully"/>
    </text>
    <text id="1.1">
        <sentence lang="prq">Sheeteni akarajetaiyeni maaroni nosaikajetzika.</sentence>
        <sentence lang="es">Buenas tardes a todos los que nos encontramos en este lugar.</sentence>
        <morphology format="" analysislevel=""/>
        <parsing format="" analysislevel=""/>
    </text>
    <text id="1.2">
        <sentence lang="prq">Iroñaaka sheetenika nonkinkitsatakoteri aparoni ashaninka ikantaitziri Apiinka.</sentence>
        <sentence lang="es">Ahora en esta tarde, voy a contarles sobre una persona llamada Apiinka.</sentence>
        <morphology format="" analysislevel=""/>
        <parsing format="" analysislevel=""/>
    </text>
    <text id="1.3">
        <sentence lang="prq">Saikaintsirika aka anampikika paitachari Metaroki.</sentence>
        <sentence lang=""/>
        <morphology format="" analysislevel=""/>
        <parsing format="" analysislevel=""/>
    </text>
    <text id="1.4">
        <sentence lang="prq">Iroñaaka ikantajiro virakocha Metraro.</sentence>
        <sentence lang=""/>
        <morphology format="" analysislevel=""/>
        <parsing format="" analysislevel=""/>
    </text>
    <text id="1.5">
        <sentence lang="prq">Yoka ashaninka osheki yaaki atziri, osheki yaaki atzirive.</sentence>
        <sentence lang=""/>
        <morphology format="" analysislevel=""/>
        <parsing format="" analysislevel=""/>
    </text>
    <text id="1.6">
        <sentence lang="prq">Yapototakiri, yapotojetakiri maaroni okanta yantavaitakantyaariri.</sentence>
        <sentence lang=""/>
        <morphology format="" analysislevel=""/>
        <parsing format="" analysislevel=""/>
    </text>
    <text id="1.7">
        <sentence lang="prq">Ari okantavitaka kameetsa, yantavaitakakiri, ipankitakakiri ishinkini, imachakite, iyaniri, ivaryantzitepaye, iyokani, irinkini.</sentence>
        <sentence lang=""/>
        <morphology format="" analysislevel=""/>
        <parsing format="" analysislevel=""/>
    </text>
    <text id="1.8">
        <sentence lang="prq">Osheki ipankitakajetakiri atziripaye arika yante ovantsi.</sentence>
        <sentence lang=""/>
        <morphology format="" analysislevel=""/>
        <parsing format="" analysislevel=""/>
    </text>
    <text id="1.9">
        <sentence lang="prq">Ityaankiri aparopaye: “Aviroka antatsini ovantsi, aviroka jatatsini antamiki, pamini vatsatsi avyari.”</sentence>
        <sentence lang=""/>
        <morphology format="" analysislevel=""/>
        <parsing format="" analysislevel=""/>
    </text>
    <text id="1.10">
        <sentence lang="prq">Ari okantajetakari, kooyapaye iroo kotsivaitatsini, eentsipaye ontzimatye yamitakoteri irora inkoyavaite.</sentence>
        <sentence lang=""/>
        <morphology format="" analysislevel=""/>
        <parsing format="" analysislevel=""/>
    </text>
    <text id="1.11">
        <sentence lang="prq">Ikanta iyoka Apiinkaka osheki inintavitakari, poñaashitaka avisaki osheki osarentsi.</sentence>
        <sentence lang=""/>
        <morphology format="" analysislevel=""/>
        <parsing format="" analysislevel=""/>
    </text>
</parallel>
